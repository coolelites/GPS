#ifndef WLIST_H
#define WLIST_H 
using namespace std;

class Waylist_node {

  private:
    Waylist_node* next;
    Waylist_node* prev;

    float x;
    float y;
    int count;
    
    Waylist_node* head;
    Waylist_node* back;
    
   public:
      Waylist_node();
      ~Waylist_node();
      void add();
      void remov();
      void clr();
};

#endif
